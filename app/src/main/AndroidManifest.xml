<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.siswa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Siswa"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activites.SettingActivity"></activity>
        <activity android:name=".activites.lihat_detail_siswa" />
        <activity android:name=".activites.lihatSiswa" />
        <activity android:name=".activites.tambahSiswa" />
        <activity android:name=".activites.MainActivity" />
        <activity android:name=".activites.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>